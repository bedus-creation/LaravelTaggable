{
    "name": "aammui/laravel-taggable",
    "description": "Assign Category and Tags to a model",
    "license": "MIT",
    "authors": [
        {
            "name": "Bedram Tamang",
            "email": "tmgbedu@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "^5.8|^6.0|^7.0",
        "illuminate/support": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Aammui\\LaravelTaggable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aammui\\LaravelTaggable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aammui\\LaravelTaggable\\LaravelTaggableServiceProvider"
            ]
        }
    }
}